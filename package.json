{
  "name": "projeto-empreendedor-frontend2",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"ng serve --port 80\" \"json-server --watch src/mocks/data.json --routes src/mocks/routes.json\"",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
